<?xml version="1.0"?>
<sdf version="1.6">
  <world name="empty_world">
  
  <physics type="ode">
  <max_step_size>0.001</max_step_size>
  <real_time_update_rate>500</real_time_update_rate>
  </physics>
    <!-- Ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Sunlight -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Add your robot later using the spawn command -->
  </world>
</sdf>
